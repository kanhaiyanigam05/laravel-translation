{
    "name": "kanhaiyanigam05/laravel-translation",
    "description": "A tool for managing all of your Laravel translations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kanhaiya"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.20.0",
        "laravel/legacy-factories": "^1.4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.4.0",
        "phpunit/phpunit": "^12.2.6",
        "mockery/mockery": "^1.6.12"
    },
    "autoload": {
        "psr-4": {
            "Kanhaiyanigam05\\Translation\\": "src"
        }
    },
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "Kanhaiyanigam05\\Translation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanhaiyanigam05\\Translation\\TranslationServiceProvider",
                "Kanhaiyanigam05\\Translation\\TranslationBindingsServiceProvider"
            ]
        }
    }
}